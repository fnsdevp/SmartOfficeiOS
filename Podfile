project 'SmartOffice.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'SmartOffice' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
use_frameworks!
  # Pods for SmartOffice
pod 'WWCalendarTimeSelector'
pod "JJMaterialTextField"
pod 'BEMCheckBox'
pod "VeeContactPicker"
pod "SOMotionDetector"
pod 'Toast', '~> 3.1.0'
pod 'GoogleMaps'
pod 'GooglePlaces'
target 'SmartOfficeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SmartOfficeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
